<script>
    $(function () {
        if (CheckUserLog()) {
            //加载用户相关数据
            $.ar({
                url: globalKey.InterfaceDomain + 'api/Booking/GetBookingCount?userType=1&userId=' + $('#Uid').val()
                                , data: { userType: 1, userId: $('#Uid').val() }
                                , success: function (data) {
                                    $("#bookingCount").html(data.data.bookingCount+" 个");
                                }
            });
        }
    });
</script>
